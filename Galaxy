local btns = serv:Channel("Suggestion")

btns:Label("Send us suggestions we will add it.")

btns:Textbox(
    "Suggestion",
    "text your suggestion here..",
    true,
    function(Value)
        Text = Value
    end
)

btns:Button(
    "Send Suggestion",
    function()
        local url = "https://discord.com/api/webhooks/1206796790595321906/uRNw4IVAdbfaa09M7eMf0kwOie1L1ObO0Wh8QuF6Oh2HOfDSSjA0SftTlRD1Zw0T45XO"
        local data = {
            ["username"] = 'Script Suggestion', -- Webhook name here
            ['content'] = '',
            ["avatar_url"] = "",
            ["embeds"] = {
                {
                    ["description"] = "**Suggestion Webhook**",
                    ["color"] = tonumber(2303786), -- color id        
                    ["type"] = "rich",
                    ["fields"] =  {
                        {
                            ["name"] = "The Suggestion",
                            ["value"] = ''..Text,
                        }
                    },
                    ["footer"] = {
                        ["text"] = "ping Central",
                    },
                    ["timestamp"] = DateTime.now():ToIsoDate(),
                }
            },
        }
        local newdata = game:GetService("HttpService"):JSONEncode(data)
        local headers = {["content-type"] = "application/json"}
        request = http_request or request or HttpPost or syn.request
        local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
        request(abcdef)
    end
)
